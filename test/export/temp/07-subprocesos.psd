PROCESO SIN_TITULO
ESCRIBIR FUNCIONDOBLE(5);
INVOCAR NORETORNANADA(3,9);
C<-(0);
INVOCAR PORREFERENCIA(C);
ESCRIBIR C;
DIMENSION A(10),B(3,4);
INVOCAR RECIBEVECTOR(A);
INVOCAR RECIBEMATRIZ(B);
FINPROCESO
SUBPROCESO A<-FUNCIONDOBLE(B)
A<-(2*B);
FINSUBPROCESO
SUBPROCESO NORETORNANADA(A,B)
ESCRIBIR A+B;
FINSUBPROCESO
SUBPROCESO PORREFERENCIA(B POR REFERENCIA)
B<-(7);
FINSUBPROCESO
SUBPROCESO RECIBEVECTOR(V)
LEER V(1);
FINSUBPROCESO
SUBPROCESO RECIBEMATRIZ(M)
LEER M(1,1);
FINSUBPROCESO
